{
  "name": "freenom-autorenew-domains",
  "version": "1.0.2",
  "description": "A scraper built with puppeteer to renew free domains on Freenom",
  "author": "Сорок два <sorokdva.developer@gmail.com>",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "run-p build:*",
    "build:all": "tsc -d",
    "develop": "tsc-watch --noClear --onSuccess \"node ./dist/index.js\"",
    "describe": "npm-scripts-info",
    "release": "standard-version -s",
    "test": "run-p test:**",
    "test:lint": "eslint 'src/**/*.ts' --fix",
    "vulnerabilities": "snyk monitor"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint"
    ]
  },
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^18.7.13",
    "cron": "^1.8.2",
    "discord-bot-webhook": "^1.0.4",
    "discord.js": "^14.3.0",
    "dotenv": "^8.2.0",
    "fs": "0.0.1-security",
    "puppeteer": "^10.4.0",
    "request": "^2.88.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/puppeteer": "^5.4.6",
    "@types/sqlite3": "^3.1.8",
    "eslint": "^7.28.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-mocha": "^10.1.0",
    "npm-run-all": "4.1.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  }
}
